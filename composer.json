{
    "name": "stanfortonski/laravel-twofactor",
    "description": "Two-factor authentication for Laravel 7.x and 8.x via e-mail.",
    "keywords": [
        "laravel",
        "Laravel 7",
        "Laravel 8",
        "twofactor",
        "auth",
        "e-mail",
        "authentication",
        "2fa",
        "laravel-package",
        "laravel-plugin",
        "laravel-module"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stan Fortonski",
            "email": "contact@stanfortonski.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Stanfortonski\\Laraveltwofactor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stanfortonski\\Laraveltwofactor\\ServiceProvider"
            ]
        }
    }
}
